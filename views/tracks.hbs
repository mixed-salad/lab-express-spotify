<table id="tracks">
    <tr class="table-header">
        <th>Title</th>
        <th>Listen</th>
    </tr>
    {{#each tracks}}
    <tr class="each-track">
        <td>{{name}}</td>
        <td class="audio-controls"><audio controls audio src="{{preview_url}}"></audio></td>
    </tr>
    {{/each}}
</table>