<div id="album-results">
    {{#each album}}
    <div class="each-album">
        <div class="image-wrapper">
            <img src="{{images.[1].url}}" alt="{{name}}">
        </div>
        <div class="album-info">
            <h4>{{name}}</h4>
            <a href="/tracks/{{id}}">View Tracks</a>
        </div>
    </div>
    {{/each}}
</div>